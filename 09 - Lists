friends = ["Kevin", "Karen", "Jim"]

print(friends) # print whole list

print(friends[0]) #access index 0
print(friends[-1]) #access index from the back of the list
print(friends[1:]) #access index 1 onwards
print(friends[1:2]) #access index 1 and 2

friends[1] = "Mike" # modify a vaule in an array/list
